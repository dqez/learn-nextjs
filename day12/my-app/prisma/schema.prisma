generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model task {
  id        Int     @id @default(autoincrement())
  title     String
  completed Boolean @default(false)
  userId    Int

  @@index([userId], map: "Task_userId_fkey")
}

model user {
  id    Int     @id @default(autoincrement())
  email String  @unique(map: "User_email_key")
  name  String?
}
